{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strictPropertyInitialization": false,
		"lib": [
			"ESNext"
		],
		"resolveJsonModule": true,
		"typeRoots": [],
		"baseUrl": "./",
		"paths": {
			"@lib/*": [
				"lib/*"
			]
		}
	}
}